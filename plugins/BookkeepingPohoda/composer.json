{
    "name": "mapik/bookkeeping-pohoda",
    "description": "BookkeepingPohoda plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "~4.2.0",
        "mapik/pohoda-xml": "^1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "BookkeepingPohoda\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BookkeepingPohoda\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
