{
    "name": "mapik/bookkeeping-pohoda",
    "description": "BookkeepingPohoda plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0.0",
        "mapik/pohoda-xml": "^1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "BookkeepingPohoda\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BookkeepingPohoda\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
